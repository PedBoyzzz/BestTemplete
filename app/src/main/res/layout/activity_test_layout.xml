<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".test.TestLayoutActivity">

    <ImageButton
        android:id="@+id/home_icon"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/home_icon_sel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/location_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
         />

    <ImageView
        android:id="@+id/settings_icon"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/settings_icon_sel"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="671dp" />

    <ImageButton
        android:id="@+id/about_icon"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/about_icon_sel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/settings_icon"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageButton
        android:id="@+id/location_icon"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/location_icon_sel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/settings_icon"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/home_icon"
        app:layout_constraintTop_toTopOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>